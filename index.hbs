                                </a>
                                <p class="overflowClip_2">{{description}}</p>
                                {{#if tags}}<p class="overflowClip_3">#{{join (split tags ',') ' #'}}</p>{{/if}}
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
            <br />
            <!--END {{category}} -->
            {{/inline}}

            {{#each categories}}
            {{#if categories}}
                {{#each categories}}
                    {{~> tagPartial category=category links=links}}
                {{/each}}
            {{else}}
                {{~> tagPartial category=category links=links}}
            {{/if}}
            {{/each}}

            <!-- Main Footer -->
